{
  "name": "bom",
  "version": "1.0.0",
  "description": "Bill of Materials (BOM) generation from KiCad schematic files",
  "author": "jBOM Development Team",
  "entry_points": {
    "workflows": [
      "bom.generate"
    ],
    "cli_commands": [
      "bom"
    ]
  },
  "dependencies": {
    "kicad_reader": ">=1.0.0",
    "fabricators": ">=1.0.0"
  },
  "file_types": {
    "input": [".kicad_sch", ".sch"],
    "output": [".csv", ".xlsx", ".json"]
  }
}
